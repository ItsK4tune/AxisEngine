# --- RESOURCES (Load trước) ---
# LOAD_SHADER <name> <vs_path> <fs_path>
LOAD_SHADER animationShader resources/shaders/anim_model.vs resources/shaders/anim_model.fs
LOAD_SHADER modelShader resources/shaders/model.vs resources/shaders/anim_model.fs
LOAD_SHADER uiShader resources/shaders/ui.vs resources/shaders/ui.fs
LOAD_SHADER textShader resources/shaders/text.vs resources/shaders/text.fs
LOAD_SOUND cuayo resources/audios/cuayo.wav

# LOAD_MODEL <name> <path>
LOAD_MODEL playerModel resources/objects/player/Dying.fbx
LOAD_MODEL humanModel resources/objects/player/SittingLaughing.fbx

# LOAD_ANIMATION <name> <model_name> <path>
LOAD_ANIMATION dyingAnim playerModel resources/objects/player/Dying.fbx
LOAD_ANIMATION laughAnim humanModel resources/objects/player/SittingLaughing.fbx

# LOAD_FONT <name> <path> <size>
LOAD_FONT arialFont resources/fonts/time.ttf 48

